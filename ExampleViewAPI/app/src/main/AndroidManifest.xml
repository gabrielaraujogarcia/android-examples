<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.minitagbrasil.exampleviewapi" >

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".ExampleCustonResources"/>
        <activity android:name=".ExampleTextViewAndEditText"/>
        <activity android:name=".ExampleAutoCompleteTextView"/>
        <activity android:name=".ExampleButtonImageButton"/>
        <activity android:name=".ExampleCheckBoxToggleButton"/>
        <activity android:name=".ExampleRadioButton"/>
        <activity android:name=".ExampleSpinner"/>
        <activity android:name=".ExampleProgressDialog"/>

    </application>

</manifest>
